/**
 * @file nodes.css
 * Basic styles for nodes.
 */

 
 
 
/**********************************************************
 *                      Nodes
 **********************************************************/
#genesis-1c {
    background-image: url("../images/background.gif");
    background-position: center top;
    background-repeat: repeat-x;
    color: #79797B;
    font-family: Arial,Helvetica,sans-serif;
    font-size: 9pt;
    height: 100%;
    letter-spacing: 0;
    line-height: 1.3em;
    text-align: left;
}

/**********************************************************
 *                      Regions
 **********************************************************/

 
 /*** MENU BARRA***/
#menu-barra{
	background-color: transparent;
    bottom: 6px;
    display: inline-block;
    font-family: Arial,Helvetica,sans-serif;
    font-size: 8pt;
    height: auto;	    
    position: absolute;
    right: 0;
    width: auto;
    z-index: 50;
}
#menu-barra h2.block-title{
	display:none;
}
#menu-barra ul{
	
}

#menu-barra li.leaf{
	list-style: none;
	float: left;
    margin-left: 5px;
    position: relative;
    text-transform: uppercase;
	
}
#menu-barra li.leaf a{
	padding:7px;
	background-color:#B2290C;
	color:#FFF;
}

#menu-barra li a:hover{
	padding:7px;
	background-color:#E8BEB6;
	color:#FFF;
	text-decoration:none;
}

/**********************************************************
 *                      Blocks
 **********************************************************/
 #sidebar-right h2.block-title {
	margin-top:15px;
	background-color:#B2290C;
	border-bottom: 5px solid #FFFFFF;
    color: #FFFFFF;
    font-size: 11pt;
    font-weight: normal;
    padding: 5px 0 5px 10px;
 }
 #sidebar-right div.block-content{
	background-color:#E8BEB6;
	padding: 5px;
	
 }
 
